package com.bridgelabz.designpatterns.factory;

public class PC extends Computer {
	 private String ram;
	 private String hdd;
	 private String cpu;
	 
	 public PC(String ram, String hdd, String cpu) {
	 	this.ram=ram;
	 	this.hdd=hdd;
	 	this.cpu=cpu;
	}

	public String getRam() {
		return this.ram;
	}

	public String getHdd() {
		return this.hdd;
	}

	public String getCpu() {
		return this.cpu;
	}

}

	