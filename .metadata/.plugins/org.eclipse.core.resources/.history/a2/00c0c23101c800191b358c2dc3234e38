package com.bridglabz.basicjavaSecA;

public class FactorialUsingRecurssion {
		int fact=1;
	public static void main(String[] args) {
		int no =5;
		factoril f=new factoril();
		f.calcFacta(int no);
			
		}
		int calcFacta(int no)
		{
			if (no>1)
			{
				fact*=no;
				calcFacta(no-1);
				return fact;
			}
			
	}
	

}
