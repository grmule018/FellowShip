package com.bridglabz.Algorithm;
import java.util.Scanner;
public class MidElementOfArray {

	public static void main(String[] args) {
	int a[]= {2,4,5,6,7};
	int start=0; 
	int end=a.length-1; 
		    
		    int mid = (start + end)/2;
		    
	System.out.println(" Position= "+mid);
	System.out.println(a[mid]);

}
}