package com.bridglabz.datastructures2;

public class LinkListInt {

	public static void main(String[] args) throws java.lang.Exception {
		
		LinkedListInt A =new LinkedListInt();
		
		A.addAtBegin(10);
		A.addAtBegin(20);
		A.addAtBegin(46);
		A.addAtBegin(15);
//		A.addAtEnd(50);
//		A.addAtEnd(21);
//		A.addAtPos(5, 3);
//		A.display();
//		A.isEmpty();
//		A.addAtPos(9, 4 );
//		A.deleteAtBegin();
//		A.deleteAtEnd();
		A.deleteAtPos(3);
		
		System.out.println(" Element at 2nd position : " + A.elementAt(2));
		
		//Print search element
		System.out.println(" Searching element location : " + A.search(15));
		
		A.display();
		
		System.out.println("size "+A.size);
	}

}
