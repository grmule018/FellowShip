/******************************************************************************
 *  Compilation:  javac -d bin Person.java
 *  Execution:    java -cp bin com.bridgelabz.oops.Person.java n
 *  
 *  Purpose: Creates a plain old java object to achieve encapsulation and for
 *  		 reusability of the code
 *
 *  ******************************/

package com.grm.oops.addressbook;

public class Person {
	private String firstName;
	private String lastName;
	private long contactNo;
	private Address address;
	
	public Person(String firstName2, String lastName2, String address2, String city, String state, String zip,
			String phone) {
		
	}

	public Address getAddress() {
		return address;
	}

	public void setAddress(Address address) {
		this.address = address;
	}

	public long getContactNo() {
		return contactNo;
	}
	public void setContactNo(long contactNo) {
		this.contactNo = contactNo;
	}
	public String getFirstName() {
		return firstName;
	}
	public void setFirstName(String firstName) {
		this.firstName = firstName;
	}
	public String getLastName() {
		return lastName;
	}
	public void setLastName(String lastName) {
		this.lastName = lastName;
	}
	
}
